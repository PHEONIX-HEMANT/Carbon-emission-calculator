{
  "manifest_version": 3,
  "name": "Carbon emission tracking Extension",
  "description": "Carbon Emmision Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["app.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "default_icon.png"
  }
}
